pipeline {
    agent none
    stages {
        stage('Build') {
            agent any
            steps {
            for (Integer x=1; x<=2; x++) {
            println(":>>>>>>>>>>>>")
            println(pwd())
            echo ">>>>>>>>>>"
            jobDsl(targets: '.jenkins/generate_tests.groovy',
                removedJobAction: 'DELETE',
                removedViewAction: 'DELETE',
                lookupStrategy: 'SEED_JOB',
                additionalParameters: [jobName: 'java-gradle-' + x.toString() , realpwd: pwd()]
                )
            }
            }
        }
    }
}
