    agent none
    stages {
        stage('Build') {
            agent any
            steps {
            jobDsl(targets: '.jenkins/generate_tests.groovy',
                removedJobAction: 'DELETE',
                removedViewAction: 'DELETE',
                lookupStrategy: 'SEED_JOB',
                additionalParameters: [message: 'Hello from pipeline', credentials: 'SECRET']
                )
            }
        }
    }
}
